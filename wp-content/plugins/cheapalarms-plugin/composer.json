{
    "name": "cheapalarms/platform-bridge",
    "description": "WordPress plugin for CheapAlarms platform integration",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4",
        "stripe/stripe-php": "^13.0"
    },
    "autoload": {
        "psr-4": {
            "CheapAlarms\\Plugin\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    }
}
